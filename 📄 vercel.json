{
  "routes": [
    { "src": "/(.*)\\.(css|js|mp3|png|jpg|jpeg|webp|svg|ico|json|txt)", "dest": "/$1.$2" },
    { "src": "/(ciclos|diag)\\.html", "dest": "/$1.html" },
    { "src": "/.*", "dest": "/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)\\.(css|js)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000" }
      ]
    }
  ]
}
