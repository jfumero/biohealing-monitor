{
  "version": 2,
  "name": "biohealing-monitor",
  "routes": [
    { "src": "/(.*)\\.(css|js|mp3|png|jpg|jpeg|gif|svg|ico|json|html)", "dest": "/$1.$2" },
    { "src": "/ciclos", "dest": "/ciclos.html" },
    { "src": "/$", "dest": "/index.html" },
    { "src": "/(?!.*\\.).*", "dest": "/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)\\.(css|js)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}
